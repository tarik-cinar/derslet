<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OgretmenKisaSinavSonuclar">

    <include
        android:id="@+id/include"
        layout="@layout/ustbar1" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/ks_ogrenci0_bilgi"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/kare_10_0_10_0_kenar"
                android:fontFamily="sans-serif"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="Öğrenci Numarası\nÖğrenci isim soyisim"
                android:textColor="@color/anaRenk"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/ks_ogrenci0_sonuc"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/ks_ogrenci0_sonuc"
                android:layout_width="65dp"
                android:layout_height="0dp"
                android:layout_marginEnd="25dp"
                android:background="@drawable/kare_0_10_0_10"
                android:backgroundTint="@color/anaRenk"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:text="5/5"
                android:textColor="@color/beyaz"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/ks_ogrenci0_bilgi"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/ks_ogrenci0_bilgi"
                app:layout_constraintTop_toTopOf="@+id/ks_ogrenci0_bilgi" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>